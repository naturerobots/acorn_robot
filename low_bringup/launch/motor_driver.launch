<launch>
    <include file="$(find cornholio2_differential_drive)/launch/wheel_motors.launch" />

    <node pkg="cornholio2_differential_drive" type="cornholio2_differential_drive_node" name="cornholio2_differential_drive_node" output="screen">
	<param name="rate" type="double" value="20.0"/>
	<param name="timeout" type="double" value="0.5" />
        <param name="wheel_radius" type="double" value="0.15"/>
        <param name="axis_length" type="double" value="0.4" />
        <param name="publish_tf" type="bool" value="true" />
        <param name="odom_frame_id" type="string" value="odom" />
	<param name="base_frame_id" type="string" value="base_footprint" />
	<param name="turning_adaptation" type="double" value="0.8" />
    </node>
</launch>
